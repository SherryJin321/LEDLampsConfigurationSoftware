                <TabItem Header="{DynamicResource LangsLogin}" FontSize="18" Background="AliceBlue" Height="28" Width="150" Margin="-5,0,5,0" Padding="0" x:Name="Login">
                    <Canvas>
                        <Label  Content="{DynamicResource LangsDeveloperModeUserName}" Canvas.Left="50" Canvas.Top="50" FontSize="16" x:Name="DeveloperModeUserName"/>
                        <Label  Content="{DynamicResource LangsDeveloperModePassword}" Canvas.Left="50" Canvas.Top="100" FontSize="16" x:Name="DeveloperModePassword"/>
                        <Border BorderBrush="#FF0A51A1" CornerRadius="5" BorderThickness="1" Height="40" Canvas.Left="210" Canvas.Top="50" Width="150">
                            <TextBox x:Name="UserName" Height="40" TextWrapping="Wrap" Width="150" Background="{x:Null}" BorderThickness="0" Padding="10,8,0,0" FontSize="16" />
                        </Border>
                        <Border BorderBrush="#FF0A51A1" CornerRadius="5" BorderThickness="1" Height="40" Canvas.Left="210" Canvas.Top="100" Width="150">
                            <PasswordBox x:Name="Password" Height="40" Width="150" Background="{x:Null}" BorderThickness="0" Padding="10,8,0,0" FontSize="16" PasswordChar="*"/>
                        </Border>
                        <Border BorderBrush="#FF0A51A1" CornerRadius="5" BorderThickness="1" Height="40" Canvas.Left="50" Canvas.Top="170" Width="130" Background="AliceBlue">
                            <Button x:Name="LogIn" Content="{DynamicResource LangsLogIn}" Background="{x:Null}" BorderThickness="0" Click="LogIn_Click"/>
                        </Border>
                        <Border BorderBrush="#FF0A51A1" CornerRadius="5" BorderThickness="1" Height="40" Canvas.Left="50" Canvas.Top="230" Width="130" Background="AliceBlue">
                            <Button x:Name="LogOut" Content="{DynamicResource LangsLogOut}"  Background="{x:Null}" BorderThickness="0" Click="LogOut_Click"/>
                        </Border>
                    </Canvas>
                </TabItem>
                <TabItem Header="{DynamicResource LangsLanguageSetting}" FontSize="18" Background="AliceBlue" Height="28" Width="150" Margin="-5,0,5,0" Padding="0" x:Name="LanguageSetting">
                    <Canvas>
                        <Border BorderBrush="#FF0A51A1" BorderThickness="1" CornerRadius="5" Width="150" Height="40" Canvas.Left="210" Canvas.Top="50" Background="{x:Null}">
                            <ComboBox x:Name="LanguageSelect" Width="146" Height="36" BorderThickness="0" FontSize="16" MaxDropDownHeight="130" HorizontalAlignment="Left" Margin="1,1,0,1" Padding="10,8,0,0" SelectedIndex="0">
                                <ComboBoxItem>中文</ComboBoxItem>
                                <ComboBoxItem>English</ComboBoxItem>
                            </ComboBox>
                        </Border>
                        <Label Content="{DynamicResource LangsLanguageSelect}" FontSize="18" Canvas.Left="50" Canvas.Top="50"/>
                        <Border BorderBrush="#FF0A51A1" BorderThickness="1" CornerRadius="5" Width="130" Height="40" Canvas.Left="50" Canvas.Top="170" Background="AliceBlue">
                            <Button x:Name="ConfirmLanguageSetting" Content="{DynamicResource LangsConfirmLanguageSetting}" Click="ConfirmLanguageSetting_Click" />
                        </Border>
                    </Canvas>
                </TabItem>

            </TabControl>
        </Canvas>
    </Grid>
</Window>
    
